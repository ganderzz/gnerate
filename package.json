{
  "name": "gnerate",
  "version": "0.2.0",
  "description": "Command-line tool for generating files",
  "main": "./dist/index.js",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/ganderzz/gnerate",
    "type": "git"
  },
  "bin": "./dist/index.js",
  "scripts": {
    "build": "rm -rf ./dist/* && tsc",
    "test": "jest",
    "gnerate": "tsc & node ./dist/index.js --config=example/gnerate.config.js"
  },
  "keywords": [
    "gnerate",
    "react",
    "redux",
    "terminal",
    "commandline"
  ],
  "author": "Dylan Paulus",
  "dependencies": {
    "nunjucks": "^3.1.2"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": ">=8.0.26",
    "@types/nunjucks": "^3.0.0",
    "jest": "^22.4.3",
    "prettier": "^1.12.0",
    "ts-jest": "^22.4.2",
    "typescript": "^2.8.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "testURL": "http://localhost/"
  }
}